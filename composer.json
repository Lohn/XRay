{
  "name": "p3k/xray",
  "type": "library",
  "license": "MIT",
  "homepage": "https://github.com/aaronpk/XRay",
  "description": "X-Ray returns structured data from any URL",
  "require": {
    "mf2/mf2": "^0.4",
    "ezyang/htmlpurifier": "^4.10",
    "indieweb/link-rel-parser": "0.1.*",
    "dg/twitter-php": "^4.1.2",
    "p3k/timezone": "*",
    "p3k/http": ">=0.1.11",
    "cebe/markdown": "^1.2.1",
    "p3k/picofeed": ">=0.1.39",
    "facebook/graph-sdk": "^5.5",
    "masterminds/html5": "^2.3",
    "league/route": "^5.0.0",
    "phpunit/phpunit": "^9.5.2"
  },
  "autoload": {
    "psr-4": {
      "p3k\\XRay\\": "lib/XRay"
    },
    "files": [
      "lib/helpers.php",
      "lib/XRay.php"
    ]
  },
  "require-dev": {
    "league/plates": "3.*"
  },
  "autoload-dev": {
    "files": [
      "controllers/Main.php",
      "controllers/Parse.php",
      "controllers/Token.php",
      "controllers/Rels.php",
      "controllers/Feeds.php",
      "controllers/Certbot.php"
    ]
  },
    "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/Lohn/php-graph-sdk.git"
    }
  ]
}
